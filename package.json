{
  "name": "browser-bookmark-manager",
  "version": "0.1.0",
  "description": "Find and manager your Chrome, Firefox, Safari, Opera and Edge bookmarks using the command-line",
  "keywords": ["browser", "bookmark", "chrome", "firefox", "safari", "opera", "edge", "search", "cli"],
  "bin": {
    "bbm": "bin/bbm.js"
  },
  "preferGlobal": true,
  "scripts": {
    "start": "node_modules/.bin/babel-node bin/bbm.js",
    "bundle": "rm -rf dist/ && babel . --out-dir dist --ignore node_modules && cp package.json dist/"
  },
  "author": "Alexander Zigelski <hedgehog@ali.dj> (http://ali.dj)",
  "repository": "dj-hedgehog/browser-bookmark-manager",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "fuse.js": "^2.4.1",
    "ini": "^1.3.4",
    "inquirer": "^1.1.2",
    "sql.js": "^0.3.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.11.5"
  },
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs",
      "transform-async-to-generator"
    ]
  }
}
